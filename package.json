    "@vue/test-utils": "^1.1.1",
    "cross-env": "^7.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.11"
  },
  "repository": {
