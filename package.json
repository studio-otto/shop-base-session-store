{
  "name": "@studio-otto/session-store",
  "version": "2.0.1",
  "description": "Adds centralized data store and getters for shopify nuxt app",
  "main": "dist/session-store.ssr.js",
  "browser": "dist/session-store.esm.js",
  "module": "dist/session-store.esm.js",
  "unpkg": "dist/session-store.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "test": "jest",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
